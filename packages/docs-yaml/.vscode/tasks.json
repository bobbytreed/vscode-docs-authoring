// See https://go.microsoft.com/fwlink/?LinkId=733558
// for the documentation about the tasks.json format
{
	"version": "2.0.0",
	"name": "npmTask",
	"command": "npm",
	"args": ["run"],
	"tasks": [
		{
			"label": "webpack",
			"type": "shell",
			"args": [
				"run",
				"webpack"
			],
			"problemMatcher": [
				{
					"pattern": [
						{
							"regexp": ".",
							"file": 1,
							"location": 2,
							"message": 3
						}
					],
					"background": {
						"activeOnStart": true,
						"beginsPattern": ".",
						"endsPattern": "."
					}
				}
			],
			"group": "build"
		},
		{
			"label": "mocha",
			"type": "shell",
			"args": [
				"run",
				"compile"
			],
			"problemMatcher": [],
			"group": "test"
		}
	]
}
